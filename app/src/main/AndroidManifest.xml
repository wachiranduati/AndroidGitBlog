<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="android.example.androidgitblog">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".DevActivity"
            android:label="@string/title_activity_dev"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity
            android:name=".PageThreeActivity"
            android:label="@string/title_activity_page_three"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".PageTwoActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>